{
  "name": "gitbook-plugin-i18n-upgrade",
  "version": "2.0.0",
  "description": "Upgraded i18n for AppAdhoc Gitbook",
  "main": "index.js",
  "scripts": {
    "test": "exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jasminexie/gitbook-plugin-i18n-upgrade.git"
  },
  "keywords": [
    "gitbook",
    "i18n",
    "plugin"
  ],
  "author": "Bingyang Xie",
  "license": "ISC",
  "engines": {
    "gitbook": ">=2.4.0"
  },
  "gitbook": {
    "properties": {
      "Lang1": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "title": "Display name of language 1.",
            "default": "English"
          },
          "url": {
            "type": "string",
            "title": "Folder name of language 1.",
            "default": "en"
          },
          "include": {
            "default": "[]",
            "title": "(Array) Paths to show button of Lang2. Will show button on all Lang1 pages if not set.",
            "type": "string"
          }
        }
      },
      "Lang2": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "title": "Display name of language 2.",
            "default": "中文"
          },
          "url": {
            "type": "string",
            "title": "Folder name of language 12.",
            "default": "zh"
          },
          "include": {
            "default": "[]",
            "title": "(Array) Paths which show button of Lang1. Will show button on all Lang2 pages if not set.",
            "type": "string"
          }
        }
      }
    }
  }
}
